install.packages("MCMCpack")
library(MCMCpack)
t.sampler <- function(draws, nu, mu, sigma2){
lambda <- rinvgamma(draws, shape = nu/2, scale = nu/2)
out <- rnorm(draws, mu, sqrt(lambda*sigma2))
out
}
test <- t.sampler(1000, 3, 0, 2)
test2 <- rt(1000, 3, ncp = 0)
plot(density(test))
lines(density(test2))
?rt
test2 <- rt(1000, 3)
plot(density(test))
lines(density(test2))
test <- t.sampler(1000, 3, 0, 1)
test2 <- rt(1000, 3)
plot(density(test))
lines(density(test2))
test <- t.sampler(1000, 3, 0, 1)
test2 <- rt(1000, 3)
plot(density(test))
lines(density(test2))
ks.test(test, test2)
